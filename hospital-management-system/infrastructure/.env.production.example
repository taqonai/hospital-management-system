# Production Environment Variables
# Copy this file to .env.production on your production server
# Update all values with your actual credentials

# Database
DB_PASSWORD=your_secure_database_password

# JWT Secrets (use long random strings in production)
JWT_SECRET=your_secure_jwt_secret_min_32_chars
JWT_REFRESH_SECRET=your_secure_refresh_secret_min_32_chars

# Twilio WhatsApp Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# Frontend API URL (optional, defaults to /api/v1)
VITE_API_URL=/api/v1
